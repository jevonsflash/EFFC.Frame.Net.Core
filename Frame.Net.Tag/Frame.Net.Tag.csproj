<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <Company>EFFC@ItTrending</Company>
    <Authors>YCH</Authors>
    <Description>E.F.F.C.Frame Tag Lib</Description>
    <Copyright>Copyright © ItTrending  2017</Copyright>
    <AssemblyVersion>3.5.0.0</AssemblyVersion>
    <FileVersion>3.5.0.0</FileVersion>
    <PackageTags>EFFC</PackageTags>
    <PackageReleaseNotes>Efficiency Focus For Core（效率、专注为核心）框架应用于.Net Core第一版发布
本包为EFFC框架的标签定义库，需要EFFC.Frame.Base、EFFC.Frame.Data、EFFC.Frame.Global的支撑</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\Frame.Net.Release\</OutputPath>
    <DocumentationFile>..\Frame.Net.Release\netcoreapp1.1\Frame.Net.Tag.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Core\ContextPaserCollection.cs" />
    <Compile Remove="Core\ContextTextCollection.cs" />
    <Compile Remove="Core\PaserCollection.cs" />
    <Compile Remove="Core\TagDataCollection.cs" />
    <Compile Remove="Core\TextCollection.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Frame.Net.Base\Frame.Net.Base.csproj" />
    <ProjectReference Include="..\Frame.Net.Data\Frame.Net.Data.csproj" />
    <ProjectReference Include="..\Frame.Net.Global\Frame.Net.Global.csproj" />
  </ItemGroup>

</Project>